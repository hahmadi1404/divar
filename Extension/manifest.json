{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage","tabs","debugger"],

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2,
    "content_scripts":[{
        "matches": ["*://*/*"],
        "js":["jquery-3.4.1.min.js","content.js"],
        "css":["content.css"]
    }]
  }